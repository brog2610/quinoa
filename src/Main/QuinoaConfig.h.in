#ifndef QuinoaConfig_h
#define QuinoaConfig_h

namespace Quinoa {

// Variables generated by CMake
#define QUINOA_EXECUTABLE            "@QUINOA_EXECUTABLE@"
#define QUINOA_VERSION               "@QUINOA_MAJOR_VER@.@QUINOA_MINOR_VER@"
#define QUINOA_RELEASE               "@QUINOA_RELEASE@"
#define QUINOA_GIT_COMMIT            "@GIT_SHA1@"
#define QUINOA_CONFIGURATION         "@QUINOA_PLATFORM@"
#define QUINOA_THIRD_PARTY_PREFIX    "@QUINOA_THIRD_PARTY_PREFIX@"
#define QUINOA_COMPILER              "@QUINOA_COMPILER@"
#define QUINOA_BUILD_TYPE            "@QUINOA_BUILD_TYPE@"
#define QUINOA_BUILD_DATE            "@QUINOA_BUILD_DATE@"
#cmakedefine DEBUG 1

// Selected mass model:
#cmakedefine QUINOA_BETA 1

// Selected hydrodynamics model:
#cmakedefine QUINOA_SLM 1
#cmakedefine QUINOA_GLM 1

// Selected mix model:
#cmakedefine QUINOA_DIRICHLET 1
#cmakedefine QUINOA_GENERALIZED_DIRICHLET 1

}

#endif // QuinoaConfig_h
